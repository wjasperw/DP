   !do !j = 1 !to 3
      If(!concat(Seen_R,!i,.4) = 0) !concat(Seen_RA,!i,.,!j) = !concat(Seen_R,!i,.,!j).
   !doend
   Var Labels !concat(Seen_RA,!i,.1) "Seen in theater" !concat(Seen_RA,!i,.2) "Seen elsewhere (e.g., on DVD/Blu-ray/streaming)" !concat(Seen_RA,!i,.3) "Have heard of, but not seen".
!doend
!EndDefine.

